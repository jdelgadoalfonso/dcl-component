{
  "version": "2.3.4",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "listFiles": false,
    "noLib": false,
    "noImplicitAny": true,
    "noEmitHelpers": false,
    "suppressImplicitAnyIndexErrors": true,
    "lib": [ "es6", "dom" ],
    "types": [
      "hammerjs",
      "jasmine",
      "node",
      "selenium-webdriver",
      "source-map",
      "webpack"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "files": [
    "dcl-component.ts"
  ]
}
