{
  "name": "dcl-component",
  "version": "2.2.4-2",
  "description": "angular2 dynamic loader component",
  "scripts": {
    "deploy": "NODE_ENV=production webpack -p --progress --color --optimize-minimize --optimize-dedupe --optimize-occurence-order",
    "prepublish": "gulp clean && tsc",
    "server": "webpack-dev-server --hot --inline --colors --display-error-details --display-cached",
    "start": "npm run server",
    "test": "gulp lint"
  },
  "main": "dcl-component.js",
  "typings": "dcl-component.d.ts",
  "keywords": [
    "angular2",
    "angularjs",
    "dynamic",
    "loader"
  ],
  "author": "Jose Antonio Delgado Alfonso <jose.delgado@aoifes.com>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdelgadoalfonso/dcl-component.git"
  },
  "bugs": {
    "url": "https://github.com/jdelgadoalfonso/dcl-component/issues"
  },
  "homepage": "https://github.com/jdelgadoalfonso/dcl-component#readme",
  "dependencies": {
    "@angular/common": "2.2.4",
    "@angular/compiler": "2.2.4",
    "@angular/core": "2.2.4",
    "@angular/platform-browser": "2.2.4",
    "@angular/platform-browser-dynamic": "2.2.4",
    "es6-promise": "4.0.5",
    "es6-shim": "0.35.1",
    "es7-reflect-metadata": "1.6.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "clean-webpack-plugin": "0.1.14",
    "compression-webpack-plugin": "0.3.2",
    "eslint": "3.11.1",
    "exports-loader": "0.6.3",
    "file-loader": "0.9.0",
    "gulp": "3.9.1",
    "gulp-eslint": "3.0.1",
    "gulp-ignore": "2.0.2",
    "gulp-rimraf": "0.2.1",
    "gulp-size": "2.1.0",
    "gulp-tsc": "1.2.5",
    "gulp-tslint": "7.0.1",
    "html-loader": "0.4.4",
    "markdown-loader": "0.1.7",
    "marked": "0.3.6",
    "pre-commit": "1.1.3",
    "raw-loader": "0.5.1",
    "require-dir": "0.3.1",
    "ts-loader": "1.2.2",
    "tslint": "4.0.2",
    "typescript": "2.1.1",
